<table class="table table-striped col-12">
    <thead>
    <tr>
        <th>Date</th>
        <th>Distance</th>
        <th>Time</th>
        <th>Average Speed</th>
    </tr>
    </thead>
    <tbody>
    {% for date, runEntry in diary %}
        <tr>
            <td>{{ date }}</td>
            <td>{{ runEntry.distance }} km </td>
            <td>{{ (runEntry.time / 3600)|round(2, 'floor')  }} h</td>
            <td>{{ runEntry.avgSpeed|round(2, 'floor') }} km/h</td>
            <td><button id="{{ date }}" class="delete-button">Delete</button></td>
        </tr>
    {% endfor %}
    </tbody>
</table>